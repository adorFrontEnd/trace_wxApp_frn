<!--pages/agency/agency.wxml-->
<view class='them'>
  <view class='list' wx:for='{{packList}}' wx:key="index">
    <view style='display:flex'>
      <view style='width:90px;height:112px;margin-right:10px;background:#f2f2f2'>
        <image lazy-load src="{{item.image}}" style='width:90px;height:100%'></image>
      </view>
     
    </view>
   <view class='item'>
      <view style='font-size: 15px;'>{{item.name}}</view>
      <view style='font-size: 15px;'>{{item.productBarCode}}</view>
      <view style='font-size: 15px;'>{{item.id}}</view>
      <view style='font-size: 15px;'>{{item.specification}}</view>
    </view>
  </view>

</view>