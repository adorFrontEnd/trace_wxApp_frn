/* pages/agencyChoose/agencyChoose.wxss */

.inquireBox {
  width: 94%;
  background: #f2f2f2;
  border-radius: 30px;
  height: 35px;
  line-height: 35px;
  margin: 10px;
  display: flex;
  margin-bottom: 10px;
}
input {
  width: 80%;
  border: none;
  height: 100%;
  padding-left: 17px;
}
.inquire {
  width: 20%;
  text-align: center;
  background: #09f;
  color: #fff;
  border-radius: 0 30px 30px 0;
}
.agencyList {
  display: flex;
  justify-content: space-between;
  border: 1px solid #f2f2f2;
  border-bottom: none;
  padding: 20px;
  line-height:20px;

}
.active-agencyList{
  display: flex;
  justify-content: space-between;
  border: 1px solid #f2f2f2;
  background: #f2f2f2;
  padding: 20px;
}
.goods {
  width: 94%;
  margin: 10px;
  background: #09f;
  border-radius: 30px;
  height: 43px;
  line-height: 43px;
  text-align: center;
  color: #fff;
  font-size: 16px;
  position: absolute;
  bottom: 10px;
}
.clear{
  text-align: center;
  border-top:1px solid #f2f2f2;
  padding-top:20px;
  color: #949494
}
.close1 {
  position: relative;
  width:50px;
  height: 35px;

}
.closeimg{
  width:20px;height:20px;
position: absolute;
top:7px;
left: 10px
}