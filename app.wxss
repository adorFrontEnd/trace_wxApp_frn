/**app.wxss**/
@import "./assets/css/common.wxss";
@import "./assets/css/theme.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding:30rpx 40rpx;
  box-sizing: border-box;
} 

.list-column{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
view,cover-view {
 font-size: 13px;
  color: #333;
  box-sizing: border-box;
  display: block;
  letter-spacing:normal;
  font-family:Arial, Helvetica, sans-serif;
}
cover-view{
  white-space: normal;
}